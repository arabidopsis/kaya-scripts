#%Module
proc ModulesHelp {} {
    puts stderr "setup a micromamba environment [uname machine]."
}
# for module whatis
module-whatis "setup a micromamba environment"
set root "/group/peb007/micromamba"
setenv MAMBA_ROOT_PREFIX ${root}
setenv MAMBA_EXE "${root}/bin/micromamba"
prepend-path PATH "${root}/bin"

