#%Module
  
proc ModulesHelp {} {
    puts stderr "put julia 1.10.2 on PATH for [uname machine]."

}

module-version 1.10.2 default

# for module whatis
module-whatis "Adds julia 1.10.2 to PATH\n"

# where to store julia packages
set group "/group/peb007"
setenv JULIA_DEPOT_PATH "${group}/.julia"

# julia -e 'print(Sys.BINDIR)'
prepend-path PATH "${group}/.julia/juliaup/julia-1.10.2+0.x64.linux.gnu/bin"
